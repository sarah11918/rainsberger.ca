---
  import XElement from 'astro-xelement'

  const {p:TypeItText} = XElement
---

<TypeItText 
@do={()=> 
  new TypeIt(this,{
    lifeLike:true,
    waitUntilVisible:true,
    afterComplete:(step)=>{
    step.destroy();
  }
  })
  .go()
}><slot/></TypeItText><script type="module" src="https://unpkg.com/typeit@8.1.0/dist/index.umd.js"></script>

